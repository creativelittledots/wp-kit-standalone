{
	"name": "wp-kit/standalone",
    "description": "The standalone version of wp-kit",
    "keywords": ["wpkit", "oop", "framework", "wordpress"],
    "license": "MIT",
    "require": {
        "wp-kit/core": "1.*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
	        "config/bootstrap.php",
	        "app/routes.php"
        ]
    },
    "config": {
		"preferred-install": "dist"
	}
}
